{
  "$schema": "./schema.json",
  "name": "sounds",
  "concurrency": 4,
  "cache": ".cache",
  "rootdir": "sounds",
  "outdir": "public/assets/sounds",
  "banks": {
    "common": {
      "base": "/assets/sounds/common",
      "rootdir": "sounds/common",
      "outdir": "public/assets/sounds/common",
      "media": {
        "effects": {
          "channels": 2,
          "bitrate": 64,
          "localization": []
        }
      }
    },
    "unicorn": {
      "base": "/assets/sounds/unicorn",
      "rootdir": "sounds/unicorn",
      "outdir": "public/assets/sounds/unicorn",
      "extends": "common",
      "media": {
        "music": {
          "channels": 2,
          "bitrate": 80,
          "localization": []
        },
        "effects": {
          "channels": 2,
          "bitrate": 64,
          "localization": []
        },
        "voice": {
          "channels": 1,
          "bitrate": 32,
          "localization": ["*"]
        }
      }
    }
  },
  "legacy_support": true
}
